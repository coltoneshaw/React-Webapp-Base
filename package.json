{
  "name": "react-webapp-base",
  "version": "1.0.0",
  "description": "React webapp base including Typescript, Jest, ESlint, and more.",
  "main": "./release/index.js",
  "author": "Colton Shaw",
  "license": "MIT",
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "check-lint": "eslint --ext .js,.jsx,.tsx,.ts . --quiet --cache",
    "fix-lint": "eslint --ext .js,.jsx,.tsx,.ts . --quiet --cache --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules NODE_ENV='development' npx jest --coverage",
    "production": "webpack  --config ./config/webpack.prod.js",
    "dev": "webpack serve --config ./config/webpack.dev.js --stats-error-details"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.2.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-environment-node": "^28.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "ts-jest": "^28.0.2"
  }
}
